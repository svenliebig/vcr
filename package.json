{
  "name": "vcr",
  "version": "0.9.16",
  "private": true,
  "homepage": "http://tv.slyox.de",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.16.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "firebase": "^4.5.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.2",
    "gulp-watch": "^4.3.11",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "moment": "^2.18.1",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "rxjs": "^5.4.3",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "gulp less",
    "watch-css": "gulp less-watch",
    "start-js": "node scripts/start.js",
    "build-js": "node scripts/build.js",
    "start": "npm run build-css && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run build-js",
    "testv": "npm run build-css && node scripts/test.js --env=jsdom --verbose",
    "testw": "npm run build-css && node scripts/test.js --env=jsdom --verbose --watch",
    "test": "npm run build-css && node scripts/test.js --env=jsdom",
    "testc": "npm run build-css && node scripts/test.js --coverage --env=jsdom --forceExit"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "raf": "^3.4.0"
  }
}
